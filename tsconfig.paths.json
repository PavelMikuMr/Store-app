{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*", "./dist/*", ""],
      "@pages/*": ["src/pages/*"],
      "@components/*": ["src/components/*"],
      "@components_i": ["./src/components/index"],
      "@type/*": ["./types/*"],
      "@type_i": ["./types/index"],
      "public/*": ["public/*"],
      "@fields_i": ["./src/components/fields/index"],
      "@assets/*": ["./src/assets/*"],
      "@hooks/*": ["./src/hooks/*"],
      "@styles/*": ["./src/styles/*"],
      "@common/*": ["./src/styles/common/*"],
      "@views/*": ["./src/styles/views/*"]
    }
  },
  "include": ["**/*.*"]
}
